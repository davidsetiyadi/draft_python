<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
<template id="sgeede_liquidity_report.sgeede_liquidity_reports">
    <t t-call="report.html_container">
        <t t-call="report.external_layout">
            <div class="page">
                <h2>Liquidity Report</h2>

                <div class="row mt32">
                    <div class="col-xs-3">
                        <strong>Company</strong>:<br/>
                        <span t-esc="res_company.name"/>
                    </div>         
                    <div class="col-xs-3">
                        <strong>Print Date</strong>:<br/>
                        <span t-esc="formatLang(time.strftime('%Y-%m-%d'),date=True)"/>
                    </div>
                    <div class="col-xs-3">
                        <strong>Start Period</strong>:<br/>
                        <span t-esc="formatLang(data['form']['date_from'],date=True)"/> 
                    </div>
                    <div class="col-xs-3">
                        <strong>End Period</strong>:<br/>
                        <span t-esc="formatLang(data['form']['date_to'],date=True)"/> 
                    </div> 
                </div>                
                <t t-raw="get_view(data['form'])"/>              
            </div>
        </t>
    </t>
</template>
</data>
</openerp>